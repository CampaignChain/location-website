{
    "name": "campaignchain/location-website",
    "description": "Simple module that connects a Website and to define its pages as CampaignChain Locations.",
    "keywords": ["website", "campaignchain"],
    "type": "campaignchain-location",
    "homepage": "http://www.campaignchain.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "CampaignChain Inc.",
            "email": "info@campaignchain.com",
            "homepage": "http://www.campaignchain.com"
        }
    ],
    "support": {
        "issues": "https://github.com/CampaignChain/campaignchain-ce/issues",
        "docs": "http://doc.campaignchain.com"
    },
    "require": {
        "campaignchain/channel-website": "dev-master"
    },
    "autoload": {
        "psr-4": { "CampaignChain\\Location\\WebsiteBundle\\": "" }
    },
    "extra": {
        "campaignchain": {
            "kernel": {
                "classes": [
                    "CampaignChain\\Location\\WebsiteBundle\\CampaignChainLocationWebsiteBundle"
                ]
            }
        }
    }
}
